prompt('will you marry me?'); 

var question = prompt;

switch (question) {
    
    case 'yes!':
        console.log('Oh thank heavans im so happy!');
        break;
        
    case 'no!':
        console.log('Oh my GD Im commiting suicide!');
        break;
        
    case 'ill think about it':
        console.log('ahhhhh there is still hope YES!!!');
        break;
        
        default:
        console.log('whats that supposed to mean?');
};
